import math
N=int(input("Ingrese un entero positivo: "))
D=N>0
F=N==0
if F==True:
    print("El cuadrado entero es 0")
elif D==True:
    G=math.sqrt(N)
    W=G//1
    I=G-W
    Q=I==0
    if Q==True:
        print("El número",N,"es el cuadrado entero de",G)
    else:
        print("El número", N, "no es el cuadrado entero de ningún número")
else:
    print("El número", N, "no es el cuadrado real de ningún número")
